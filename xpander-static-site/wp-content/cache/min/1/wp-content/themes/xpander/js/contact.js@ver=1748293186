jQuery(document).ready(function($){$("#contact-hubspot-form").submit(function(event){event.preventDefault();var hsPortalId="46490851";var formId="334ac892-3558-4410-9ed5-64596ed27643";var formUrl=`https://api.hsforms.com/submissions/v3/integration/submit/${hsPortalId}/${formId}`;var requestBody={fields:[{name:"firstname",value:$("#first_name").val()},{name:"lastname",value:$("#last_name").val()},{name:"jobtitle",value:$("#job_title").val()},{name:"company",value:$("#company_name").val()},{name:"email",value:$("#email").val()},{name:"message",value:$("#message").val()},],};$.ajax({url:formUrl,type:"POST",contentType:"application/json",data:JSON.stringify(requestBody),success:function(response){$("#status-message").text("Your message has been sent successfully!").css("color","#753cff");$("#contact-hubspot-form")[0].reset()},error:function(error){$("#status-message").text("Submission failed. Please try again.").css("color","red")},})})})